<div class="container">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <div
                    style="background-color: #005073; display: inline-block; padding: 0.3rem 2rem; color: white; font-weight: bold; border-radius: 0 20rem 0 0; margin: 0.5rem 0; font-size: 12px;">
                    <a style="color: white" href="#">Kinh doanh</a>
                </div>
                <div class="bd-example">
                    <div id="carouselKinhDoanh" class="carousel slide" data-ride="carousel">
                        <ol class="carousel-indicators">
                            <li data-target="#carouselKinhDoanh" data-slide-to="0" class="active"></li>
                            <li data-target="#carouselKinhDoanh" data-slide-to="1"></li>
                            <li data-target="#carouselKinhDoanh" data-slide-to="2"></li>
                        </ol>
                        <div class="carousel-inner">
                            {{#each m1}}
                            <div style="background-image: -webkit-linear-gradient(white 30%, black 70%)"
                                class="carousel-item {{#if active}}active{{/if}}">
                                <a style="text-decoration: none; color: inherit" href="javascript:;">
                                    <img style="opacity: 0.4; vertical-align: middle;" src="{{image_link}}" class="d-block w-100"
                                        alt="{{title}}">
                                    <div class="carousel-caption d-none d-md-block">
                                        <h6>{{title}}</h6>
                                        <p>{{mySubString2 content}}</p>
                                    </div>
                                </a>
                            </div>
                            {{/each}}
                        </div>
                        <a class="carousel-control-prev" href="#carouselKinhDoanh" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselKinhDoanh" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                </div>
            </div>
